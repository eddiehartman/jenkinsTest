pipeline {
  agent any
  stages {
    stage("Bjorn") {
      steps {
        echo "Hi Bjorn!"
      }
    }
    stage("Build") {
      steps {
        echo "Hello build"
      }
    }
    stage("Test") {
      steps {
        echo "Hello tests"
        echo "More tests"
      }
    }
    stage("Deploy") {
      steps {
        echo "Hello deploy"
      }
    }
  }
}
